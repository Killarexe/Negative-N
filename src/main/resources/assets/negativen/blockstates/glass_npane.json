{
  "multipart": [
    {
      "apply": {
        "model": "negativen:block/glass_npane_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "negativen:block/glass_npane_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "negativen:block/glass_npane_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "negativen:block/glass_npane_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "negativen:block/glass_npane_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "negativen:block/glass_npane_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "negativen:block/glass_npane_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "negativen:block/glass_npane_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "negativen:block/glass_npane_noside",
        "y": 270
      }
    }
  ]
}